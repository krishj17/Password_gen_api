{
  "version": 2,
  "name": "arch-password-generator-api",
  "builds": [
    {
      "src": "api.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api.js": {
      "maxDuration": 10
    }
  }
}